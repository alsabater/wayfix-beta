#events .calendar{
  border-collapse: collapse;
  width: 100%;
}

#events .calendar .notmonth {
  color: #CCC; 
}
#events .calendar .today { 
  background: #D7F2FF; 
}

#events .calendar td, th {
    font-family: "Lucida Grande", arial, helvetica, sans-serif;
    font-size: 10px;
    padding: 6px;
    border: 1px solid #999;
}

#events .calendar th {
    background: #DDD;
    color: #666;
    text-align: center;
    width: 14.28%;
}

#events .calendar td {
    background: #FFF;
    color: #777;
    height: 150px;
    vertical-align: top;
    font-size: 16px;
}

#events h3 {
  text-align: center;
}

#events {
    text-decoration: none;
    color: black;
    float: left;
    width: 85%;
}

#events ul {
  font-size: 11px;
  padding-left: 20px;
}

#today_events {
  float: right;
}

