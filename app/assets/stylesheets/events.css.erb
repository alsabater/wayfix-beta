#events .calendar{
  border-collapse: collapse;
  width: 100%;
}

#events .calendar .notmonth {
  color: #CCC; 
}
#events .calendar .today { 
  color: red;
}

#events .calendar td, th {
    font-family: "Lucida Grande", arial, helvetica, sans-serif;
    font-size: 10px;
    padding: 6px;
    border: 1px solid #999;
}

#events .calendar th {
    color: #666;
    text-align: center;
    width: 14.28%;
}

#events .calendar td {
    color: #777;
    height: 100px;
    vertical-align: top;
    font-size: 16px;
}

.very_heavy {
  background-color: #346178;
  color: #777;
  height: 100px;
  vertical-align: top;
  font-size: 16px;
}

.heavy {
  background-color: #5eafd8;
  color: #777;
  height: 100px;
  vertical-align: top;
  font-size: 16px;
}

.moderated {
  background-color: #87cff3;
  color: #777;
  height: 100px;
  vertical-align: top;
  font-size: 16px;
}

.light {
  background-color: #b4e1f7;
  color: #777;
  height: 100px;
  vertical-align: top;
  font-size: 16px;
}

.very_light {
  background-color: #d2edfa;
  color: #777;
  height: 100px;
  vertical-align: top;
  font-size: 16px;
}


#events h3 {
  text-align: center;
}

#events {
    text-decoration: none;
    color: black;
    float: left;
    width: 85%;
}

#events ul {
  font-size: 11px;
  padding-left: 20px;
}

#today_events {
  float: right;
}

#events_table {
  margin: 0;
  float: left;
  width: 1000px;
}

.filter {
  margin: 0;
  float: right;
}

#am_pm {
  text-align: center;
}

.alert {
  text-align: center;
  width: 250px;
  margin: auto;
  background-color: #5FB0D3;
  color: white;
  border-radius: 5px;
}

.ui-autocomplete {
  z-index: 2000;
}
